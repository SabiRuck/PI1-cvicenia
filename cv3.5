import tkinter as tk, random as rand

canvas = tk.Canvas(width=410, height=250)
canvas.pack()

def main():
    for i in range(10):
        karticka(rand.randint(50, 300), rand.randint(50, 200), "Python")



def karticka(x, y, text):
    canvas.create_rectangle(x, y, x + 100, y + 40, fill="lightgrey")
    canvas.create_text(x + 50, y + 20, text=text, font="arial 14")






main()
canvas.mainloop()
