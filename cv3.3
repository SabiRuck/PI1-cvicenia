import tkinter as tk, random as rand

canvas = tk.Canvas(width=440, height=320)
canvas.pack()

def main():
    for i in range(10):
        kruhy(rand.randint(50, 330), rand.randint(50, 210))

def kruhy(x, y):
    d = 100
    for i in range(10):
        color = rand_color()
        canvas.create_oval(x, y, x+d, y+d, fill=color)
        x += 5
        y += 5
        d -= 10

def rand_color():
    r = rand.randint(0, 255)
    g = rand.randint(0, 255)
    b = rand.randint(0, 255)

    return f"#{r :02x}{g :02x}{b :02x}"







main()
canvas.mainloop()
