import tkinter as tk

canvas = tk.Canvas()
canvas.pack()

r = 120
k = 6
d = 2 * r
x, y = 0, 0

for i in range(d // 3):
    if i % 6 == 0:
        color = "grey"
    else:
        color = "black"
    canvas.create_oval(x, y, x + d, y+d, outline=color)
    x += 3
    y += 3
    d -= 6
    if d <= 30:
        break





canvas.mainloop()
